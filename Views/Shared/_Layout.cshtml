        <script>
            function myNavFunc() {
                // If it's an iPhone..
                if ((navigator.platform.indexOf("iPhone") != -1)
                    || (navigator.platform.indexOf("iPod") != -1)
                    || (navigator.platform.indexOf("iPad") != -1))
                    window.open("maps://maps.google.com/maps?daddr=40.220970,29.07069");
                else
                    window.open("http://maps.google.com/maps?daddr=40.220970,29.07069");
            }
            function google_maps() {
                var latlng = new google.maps.LatLng(40.220970, 29.07069);
                var myOptions = {
                    zoom: 17,
                    center: latlng,
                    zoomControl: true,
                    mapTypeControl: false,
                    streetViewControl: false,
                    scrollwheel: false,
                    mapTypeId: 'roadmap'
                };
                var map = new google.maps.Map(document.getElementById("gmap"), myOptions);
                var marker = new google.maps.Marker({
                    position: latlng,
                    popup: true,
                    map: map,
                    icon: "/tema/ultimate/img/mappin.png",
                });

                marker.info = new google.maps.InfoWindow({
                    content: '<center id="mappin"><b>Çelik Kalıp Makina</b><br><span onclick="myNavFunc()" style="cursor:pointer;">Adres Tarifi Almak için Tıklayın</span></center>'
                });
                marker.info.open(map, marker);

                google.maps.event.addListener(marker, 'click', function () {
                    marker.info.open(map, marker);
                });

            }

            jQuery(document).ready(function ($) {
                google_maps();
            });
        </script>

